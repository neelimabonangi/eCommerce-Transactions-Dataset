   "source": [
    "import csv\n",
    "\n",
    "# Your lookalikes dictionary\n",
    "lookalikes = {\n",
    "    'C0001': [('C0005', 0.95), ('C0002', 0.92), ('C0003', 0.91)],\n",
    "    'C0002': [('C0001', 0.92), ('C0004', 0.89), ('C0003', 0.88)],\n",
    "    'C0003': [('C0001', 0.91), ('C0005', 0.87), ('C0004', 0.85)],\n",
    "}\n",
    "\n",
    "# Define the CSV file path\n",
    "csv_file_path = 'Lookalike.csv'\n",
    "\n",
    "# Define the header\n",
    "header = ['cust_id', 'lookalike_cust_id', 'similarity_score']\n",
    "\n",
    "# Open the CSV file in write mode\n",
    "with open(csv_file_path, mode='w', newline='') as file:\n",
    "    writer = csv.writer(file)\n",
    "    writer.writerow(header)  # Write the header row\n",
    "\n",
    "    # Iterate over the lookalikes dictionary\n",
    "    for cust_id, lookalike_list in lookalikes.items():\n",
    "        for lookalike_cust_id, similarity_score in lookalike_list:\n",
    "            # Write each lookalike entry as a new row\n",
    "            writer.writerow([cust_id, lookalike_cust_id, similarity_score])\n",
    "\n",
    "print(f\"CSV file '{csv_file_path}' has been created successfully.\")\n"
   ]
  }
 ],
